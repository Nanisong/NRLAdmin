@model NRLAdmin.Models.Ladder


@{
    ViewBag.Title = "Index";
}

<h2>Ladder</h2>

<table class="table">
    <tr>
        
        <th>TeamName</th>
        <th>Played</th>
        <th>Points</th>
        <th>Wins</th>
        <th>Draws</th>
        <th>Losts</th>
    </tr>

    @for (var i = 0; i < @Model.TeamNameList.Count; i++)
    {
        <tr>
            <th>@Model.TeamNameList[i]</th>
            <th>@Model.PlayedList[i]</th>
            <th>@Model.PointsList[i]</th>
            <th>@Model.WinsList[i]</th>
            <th>@Model.DrawsList[i]</th>
            <th>@Model.LostsList[i]</th>
        </tr>
    }


</table>
